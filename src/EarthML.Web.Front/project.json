{
  "dependencies": {
    "WebApiContrib.Core.TagHelpers.Markdown": "1.0.0",
    "Microsoft.NETCore.App": {
      "version": "1.0.1",
      "type": "platform"
    },
      "Microsoft.AspNetCore.Diagnostics": "1.1.0-alpha1-22294",
      "Microsoft.AspNetCore.Mvc": "1.1.0-alpha1-22294",
      "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0-alpha1-22294",
      "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview3-22107",
      "Microsoft.AspNetCore.Server.Kestrel": "1.1.0-alpha1-22294",
      "Microsoft.AspNetCore.StaticFiles": "1.1.0-alpha1-22294",
      "Microsoft.Extensions.Configuration.FileExtensions": "1.1.0-alpha1-22294",
      "Microsoft.Extensions.Configuration.Json": "1.1.0-alpha1-22294",
      "Microsoft.Extensions.Logging.Console": "1.1.0-alpha1-22294"
    },

  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "web.config",
      "Areas/**/*.cshtml",
      "Areas/Blog/Articles/**/*.md",
      "appsettings.json"
    ]
  },

  "scripts": {
    "prepublish": ["npm install", "grunt build"],
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }
}
